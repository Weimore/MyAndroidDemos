<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mymodeldemos">

    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:name=".application.MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/rella_app"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".NotUsedHomeActivity"
            android:configChanges="orientation|screenSize|keyboardHidden"/>
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".HomeActivity"
            android:configChanges="orientation|screenSize|keyboardHidden" />
        <activity android:name=".sample.SampleListActivity" />
        <activity android:name=".sample.SampleSectionListActivity" />
        <activity android:name=".sample.SampleMultipleListActivity" />
        <activity android:name=".sample.SampleNewMultipleListActivity" />
        <activity android:name=".sample.SampleNewListActivity" />
        <activity android:name=".NewHomeActivity"
            android:configChanges="orientation|screenSize|keyboardHidden"/>
    </application>

</manifest>